.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.clearfix::after {
  content: ' ';
  display: table;
  visibility: hidden;
  clear: both;
  height: 0px;
}

.ml20 {
    margin-left: 20rpx;
}